{"ast":null,"code":"export default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      defaultAvatar: \"https://via.placeholder.com/100\",\n      // ÈªòËÆ§Â§¥ÂÉè\n      defaultPostImage: \"https://via.placeholder.com/500x250\",\n      // ÈªòËÆ§Â∏ñÂ≠êÂõæÁâá\n      posts: [],\n      // Áî®Êà∑ÁöÑÂ∏ñÂ≠êÊï∞ÊçÆ\n      currentPage: 1,\n      // ÂΩìÂâçÈ°µÊï∞\n      pageSize: 2,\n      // ÊØèÊ¨°ËØ∑Ê±ÇÁöÑÂ∏ñÂ≠êÊï∞\n      totalPosts: 0,\n      // ÊÄªÂ∏ñÊï∞\n      loading: false,\n      // ÊòØÂê¶Ê≠£Âú®Âä†ËΩΩ\n      noMoreData: false,\n      // ÊòØÂê¶Â∑≤ÁªèÊ≤°ÊúâÊõ¥Â§öÊï∞ÊçÆ\n      formVisible: false,\n      form: {}\n    };\n  },\n  mounted() {\n    this.fetchPosts();\n  },\n  methods: {\n    // Ëé∑ÂèñÂ∏ñÂ≠ê‰ø°ÊÅØ\n    fetchPosts(page = 1) {\n      if (this.loading || this.noMoreData) return; // Â¶ÇÊûúÊ≠£Âú®Âä†ËΩΩÊàñÊ≤°ÊúâÊõ¥Â§öÊï∞ÊçÆÔºå‰∏çÂÜçËØ∑Ê±Ç\n      this.loading = true;\n      this.$request.get('/cards/selectPage', {\n        params: {\n          pageNum: page,\n          pageSize: this.pageSize\n        }\n      }).then(response => {\n        if (response.data) {\n          if (response.data.list && response.data.list.length > 0) {\n            this.posts = [...this.posts, ...response.data.list]; // ËøΩÂä†Êñ∞Â∏ñÂ≠ê\n          }\n\n          this.totalPosts = response.data.total || 0;\n          if (this.posts.length >= this.totalPosts) {\n            this.noMoreData = true; // Â¶ÇÊûúÂä†ËΩΩÂÆåÊâÄÊúâÊï∞ÊçÆÔºåÂàôÊ†áËÆ∞Ê≤°ÊúâÊõ¥Â§öÊï∞ÊçÆ\n          }\n        }\n      }).catch(error => {\n        console.error('Âä†ËΩΩÂ∏ñÂ≠êÂ§±Ë¥•:', error);\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    // ÊªöÂä®Êó∂Ëß¶Âèë\n    handleScroll() {\n      const container = this.$refs.scrollContainer;\n\n      // Ë∞ÉËØïËæìÂá∫ÊªöÂä®‰ø°ÊÅØ\n      console.log('scrollTop:', container.scrollTop, 'clientHeight:', container.clientHeight, 'scrollHeight:', container.scrollHeight);\n\n      // Âà§Êñ≠ÊòØÂê¶ÊªöÂä®Âà∞ÂÆπÂô®Â∫ïÈ