ils\">\r\n        <img :src=\"order.crop_avatar\" alt=\"Crop Avatar\" />\r\n\r\n      <p><strong>农户姓名:</strong> {{ order.farmer_name }}</p>\r\n      <p><strong>农户邮箱:</strong> {{ order.farmer_email }}</p>\r\n      <p><strong>用户名:</strong> {{ order.username }}</p>\r\n      <p><strong>用户邮箱:</strong> {{ order.user_email }}</p>\r\n      <p><strong>区域面积:</strong> {{ order.area }} 平方米</p>\r\n      <p><strong>种植状态:</strong> {{ order.status }}</p>\r\n      <p><strong>开始时间:</strong> {{ order.begin_time }}</p>\r\n      <p><strong>结束时间:</strong> {{ order.end_time }}</p>\r\n    </div>\r\n\r\n    <div class=\"actions\">\r\n      <el-button type=\"danger\" @click=\"cancelOrder\">取消订单</el-button>\r\n      <el-button\r\n          v-if=\"isFarmer\"\r\n          type=\"primary\"\r\n          @click=\"showUpdateStatusDialog = true\"\r\n      >\r\n        更改订单状态\r\n      </el-button>\r\n    </div>\r\n\r\n    <!-- 修改状态弹窗 -->\r\n    <el-dialog :visible.sync=\"showUpdateStatusDialog\" title=\"更改订单状态\">\r\n      <el-input v-model=\"newStatus\" placeholder=\"输入新的状态\"></el-input>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"showUpdateStatusDialog = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateOrderStatus\">确定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"OrderDetail\",\r\n  data() {\r\n    return {\r\n      order: [],\r\n      showUpdateStatusDialog: false,\r\n      newStatus: \"\",\r\n      isFarmer: false,\r\n      id: 0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.id = this.$route.params.id;\r\n    this.isFarmer = this.$route.params.role === \"FARMER\";\r\n    this.fetchOrderDetails(this.id);\r\n  },\r\n  methods: {\r\n    fetchOrderDetails(orderId) {\r\n      this.$request\r\n          .get(`/orders/${orderId}`)\r\n          .then((response) => {\r\n            this.order = response.data;\r\n            if (response.code != \"200\") th